<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>01 Mobile Money Agent</h:title>
    <model>
      <instance>
        <hot_01_mobile_money id="01_mobile_money">
          <start/>
          <today/>
          <deviceid/>
          <image/>
          <osm_mobile_money/>
          <meta>
            <instanceID/>
          </meta>
        </hot_01_mobile_money>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/hot_01_mobile_money/start" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/hot_01_mobile_money/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/hot_01_mobile_money/deviceid" type="string"/>
      <bind nodeset="/hot_01_mobile_money/image" type="binary"/>
      <bind nodeset="/hot_01_mobile_money/osm_mobile_money" required="true()" type="binary"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/hot_01_mobile_money/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <upload mediatype="image/*" ref="/hot_01_mobile_money/image">
      <label>Take a picture</label>
    </upload>
    <upload mediatype="osm/*" ref="/hot_01_mobile_money/osm_mobile_money">
      <label>Mobile money agent</label>
      <hint>Tag mobile money agent</hint>
      <tag key="amenity">
        <label>Amenity (required)</label>
        <item>
